#include<stdio.h>
#include<conio.h>
int main()
{
	int n,sum=1,r,t,sum1=0;
        scanf("%d",&n);
        t=n;
        while(n!=0)
        {
            r=n%10;
            sum=sum*r;
            n=n/10;
        }
        printf("%d",sum);
        while(t!=0)
        {
          r=t%10;
          sum1=sum1+r;
          t=t/10;
        }
        printf("\n%d\n",sum1);
        printf("%d",sum-sum1);

}
